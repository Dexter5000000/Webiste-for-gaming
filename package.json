{
  "name": "zenith-daw",
  "version": "1.0.0",
  "description": "A professional digital audio workstation with modern web technologies",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:secure": "doppler run -- npm run dev",
    "build": "tsc && vite build",
    "build:secure": "doppler run -- npm run build",
    "preview": "vite preview",
    "preview:secure": "doppler run -- npm run preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "scrapy:convert": "ts-node scripts/convert-scrapy-formats.ts",
    "scrapy:process": "ts-node scripts/process-scrapy-data.ts"
  },
  "keywords": [
    "daw",
    "audio",
    "music",
    "production",
    "web-audio",
    "timeline",
    "mixer"
  ],
  "author": "Zenith DAW Team",
  "license": "MIT",
  "dependencies": {
    "@honeybadger-io/js": "^6.12.3",
    "@honeybadger-io/react": "^6.1.27",
    "@tonejs/midi": "^2.0.28",
    "immer": "^10.2.0",
    "jszip": "^3.10.1",
    "lamejs": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^4.0.6",
    "wasm-media-encoders": "^0.7.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.9.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^4.0.6",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
