{
  "name": "zenith-browser",
  "version": "1.0.0",
  "description": "A minimalistic, clean browser alternative with modern design",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\""
  },
  "keywords": [
    "browser",
    "minimalistic",
    "pwa",
    "clean",
    "alternative"
  ],
  "author": "Zenith Browser Team",
  "license": "MIT",
  "dependencies": {
    "immer": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^4.0.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^4.0.6",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
